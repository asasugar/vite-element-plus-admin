{
  "name": "vite-element-plus-admin",
  "version": "0.0.11",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:debug": "vue-tsc --noEmit && vite build --debug",
    "preview": "vite preview",
    "analyzer": "vue-tsc --noEmit && vite build --mode analyzer",
    "lint": "eslint . --fix --ext .js,.ts,.vue",
    "log": "standard-version",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "deploy": "cp CNAME 404.html ./dist && gh-pages -b 'vite-element-plus-admin' -d ./dist"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": "pnpm lint"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.6",
    "@vueuse/core": "^7.6.2",
    "axios": "^0.25.0",
    "custom-json2excel": "^3.1.4",
    "default-passive-events": "^2.0.0",
    "echarts": "^5.2.2",
    "element-plus": "^1.3.0-beta.5",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.13",
    "qs": "^6.10.3",
    "resize-observer-polyfill": "^1.5.1",
    "three": "^0.137.5",
    "vditor": "^3.8.11",
    "vue": "^3.2.25",
    "vue-json-pretty": "^2.0.6",
    "vue-router": "4"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^17.0.11",
    "@types/qs": "^6.9.7",
    "@types/three": "^0.137.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-legacy": "^1.8.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.7",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "rollup": "^2.70.2",
    "rollup-plugin-visualizer": "^5.5.4",
    "standard-version": "^9.3.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.4.4",
    "unplugin-vue-components": "^0.17.17",
    "vite": "^2.9.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.29.8"
  },
  "packageManager": "pnpm@6.30.0",
  "engines": {
    "node": ">=12.2.0",
    "pnpm": ">=6"
  }
}
