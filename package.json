{
  "name": "vite-element-plus-admin",
  "version": "0.0.8",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:debug": "vue-tsc --noEmit && vite build --debug",
    "preview": "vite preview",
    "analyzer": "vue-tsc --noEmit && vite build --mode analyzer",
    "lint": "eslint . --fix --ext .js,.ts,.vue",
    "log": "standard-version",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "predeploy": "cp CNAME 404.html ./dist",
    "deploy": "gh-pages -b 'vite-element-plus-admin' -d ./dist"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": "yarn lint"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.6",
    "@vueuse/core": "^7.6.2",
    "axios": "^0.25.0",
    "echarts": "^5.2.2",
    "element-plus": "^1.3.0-beta.5",
    "mockjs": "^1.1.0",
    "qs": "^6.10.3",
    "three": "^0.137.5",
    "vditor": "^3.8.11",
    "vue": "^3.2.25",
    "vue-json-pretty": "^2.0.6",
    "vue-router": "4",
    "vuex": "^4.0.2",
    "vuex-composition-maphooks": "^1.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@types/node": "^17.0.11",
    "@types/three": "^0.137.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "standard-version": "^9.3.2",
    "typescript": "^4.4.4",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.17",
    "vite": "^2.7.13",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.29.8"
  },
  "engines": {
    "node": ">=12.2.0"
  }
}
